{
    "manifest_version": 3,
    "name": "Website Blocker",
    "description": "Block and Unblock websites",
    "version": "1.0",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/x-black-logo.png"
    }
  }